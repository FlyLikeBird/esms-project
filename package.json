{
  "private": true,
  "scripts": {
    "start": "set PORT=8001 && umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@ant-design/pro-layout": "^6.5.0",
    "@topology/core": "^0.5.31",
    "@topology/flow-diagram": "^0.3.0",
    "crypto-js": "^4.1.1",
    "echarts": "^4.7.0",
    "echarts-for-react": "^3.0.1",
    "html2canvas": "^1.1.5",
    "react": "17.x",
    "react-dom": "17.x",
    "react-pdf": "^5.5.0",
    "umi": "^3.5.13",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.13",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
